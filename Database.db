CREATE TABLE `kycinfo` (
  ID int NOT NULL auto_increment,
  PanCard varchar(50) NOT NULL,
  CancelCheque varchar(50) NOT NULL,
  AddressProof varchar(50) NOT NULL,
  HighestEducation varchar(50) NOT NULL,
  PartnerPhoto varchar(50) NOT NULL,
  MSMECertificate varchar(50) NOT NULL,
  GSTCertificate varchar(50) NOT NULL,
  PRIMARY KEY (`ID`)
);

